!Save to disk performance measurements

STRU PLOT FILE "times.plot"
   DELI ';'
   FREQ 0.
   STRU VARI NAME "CPUTIME"
      #begin PATH
      CPU = BASE:'SEQUENCE':'CPUTIME'
      CPU
      #end
   END
   STRU VARI
      NAME "REALTIMEONCPU"
      #begin PATH
      CPU = BASE:'SEQUENCE':'CPUTIME'
      TIME = BASE:'SEQUENCE':'TIME'
      IF(TIME==0.) STOP 0.0
      IF(CPU ==0.) STOP 0.0
      EFF = TIME/CPU
      EFF
      #end
   END
   
   STRU VARI NAME "cesar" MODULE 'CESAR' FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "icare" MODULE 'ICARE' FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "cpa" MODULE 'CPA' FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "medicis" MODULE 'MEDICIS' FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "sophaeros" MODULE 'SOPHAEROS' FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "dose" MODULE 'DOSE' FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "isodop" MODULE 'ISODOP' FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "DBcopy" TASK "data base duplication" FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "DBsave" TASK "save and plot" FIELD 'TIME' FILEPATH "perf.path" END
   STRU VARI NAME "Ncesar" MODULE 'CESAR' FIELD 'NCALL' FILEPATH "perf.path" END
   STRU VARI NAME "Nicare" MODULE 'ICARE' FIELD 'NCALL' FILEPATH "perf.path" END
   STRU VARI NAME "Ncpa" MODULE 'CPA' FIELD 'NCALL' FILEPATH "perf.path" END
   STRU VARI NAME "Nmedicis" MODULE 'MEDICIS' FIELD 'NCALL' FILEPATH "perf.path" END
   STRU VARI NAME "Nsophaeros" MODULE 'SOPHAEROS' FIELD 'NCALL' FILEPATH "perf.path" END
   STRU VARI NAME 'ITERASTEC'
      #begin PATH
      IFTHEN(NUMBER('ITER', BASE:'SEQUENCE' 1) <> 0)
      result=FLOAT(BASE:'SEQUENCE' 1:'ITER')
      ELSE
      result=0.0
      ENDIF
      result
      #end
   END
   STRU VARI NAME 'DT.ASTEC' PATH "BASE:'SEQUENCE' 1:'STEP'" END
   STRU VARI NAME 'DT.CESAR' PATH "BASE:'CALC_OPT' 1:'CESAR' 1:'dt_last'" END
   STRU VARI NAME 'ITERCESA'
      #begin PATH
      IFTHEN(NUMBER('ITERTOT', BASE:'CALC_OPT' 1:'CESAR' 1) <> 0)
      result=FLOAT(BASE:'CALC_OPT' 1:'CESAR' 1:'ITERTOT')
      ELSE
      result=0.0
      ENDIF
      result
      #end
   END
   STRU VARI NAME 'DT.ICARE' PATH "BASE:'CALC_OPT' 1:'ICARE' 1:'DTlast'" END
   STRU VARI NAME 'ITERICA'
      #begin PATH
      IFTHEN(NUMBER('ITERTOT', BASE:'CALC_OPT' 1:'ICARE' 1) <> 0)
      result=FLOAT(BASE:'CALC_OPT' 1:'ICARE' 1:'ITERTOT')
      ELSE
      result=0.0
      ENDIF
      result
      #end
   END
   
END
